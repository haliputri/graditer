{
    "version": 2,
    "builds": [
      {
        "src": "!(templates|static)/**/*.py",
        "use": "@vercel/python",
        "config": { "zeroConfig": true }
      },
      {
        "src": "templates/**/*.html",
        "use": "@vercel/static"
      },
      {
        "src": "static/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/aes.py" }
    ]
  }
  